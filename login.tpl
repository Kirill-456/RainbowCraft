[not-group=5]				
			<!-- Состояние аккаунта -->
			<div class="block">
				<div class="block_title">Состояние моего аккаунта</div>
				<div class="block_content">
					<div class="block_top">
						<div id="u_title"><center>Вы вошли как, <a href="{profile-link}"><span>{login}</span></a></center></div>
						<div id="u_data">
							<table>
								<tr>
									<td><b>У вас<b>:</td>
									<td>{money}</td>
								</tr>
								<tr>
									<td><b>Вы</b>:</td>
									<td>{group}</td>
								</tr>
								
							</table>
						</div>
						<div id="u_message">
							<a href="{pm-link}">Личные сообщения</a> ({new-pm} | {all-pm})
							<img src="{THEME}/images/message_icon.png" alt="">
						</div>
						<div id="u_logout" style="margin-bottom: -20px;"><a href="{logout-link}">Выход</a></div>
					</div>
				</div>
				<div class="block_footer">
				</div>
			</div>
[/not-group]